    <sql>SELECT --NULL AS LEG_ID,&#xd;&#xa;--TRIM&#x28;LS.SELLING_CARRIER&#x29; AS CARRIER_CODE,&#xd;&#xa;TRIM&#x28;LS.FLIGHT_NUM&#x29; AS FLIGHT_NUMBER,&#xd;&#xa;TRIM&#x28;LS.FROM_AIRPORT&#x29; AS BD_AIRPORT_CODE,&#xd;&#xa;TRIM&#x28;LS.TO_AIRPORT&#x29; AS OFF_AIRPORT_CODE,&#xd;&#xa;--TO_CHAR&#x28;LF.DEPARTURE_DATE,&#x27;DD-MON-YYYY&#x27;&#x29; DEPARTURE_DATE, &#xd;&#xa;TRUNC&#x28;LF.DEPARTURE_DATE&#x29; DEPARTURE_DATE, &#xd;&#xa;LF.LOGICAL_FLIGHT_ID, &#xd;&#xa;LF.SCH_DEPARTURE AS DEP_TIME_LTC,&#xd;&#xa;--CAST&#x28;FROM_TZ &#x28;CAST&#x28;LF.SCH_DEPARTURE AS TIMESTAMP&#x29;,BDP.TIMEZONE_NAME&#x29; AT TIME ZONE &#x27;UTC&#x27; AS DATE&#x29; DEP_TIME_UTC,&#xd;&#xa;CAST&#x28;SYS_EXTRACT_UTC&#x28;FROM_TZ&#x28;CAST&#x28;LF.SCH_DEPARTURE AS TIMESTAMP&#x29;,NVL&#x28;TZ_OFFSET&#x28;BDP.TIMEZONE_NAME&#x29;,&#x27;&#x2b;00&#x3a;00&#x27;&#x29;&#x29;&#x29; AS DATE&#x29; DEP_TIME_UTC,&#xd;&#xa;LF.SCH_ARRIVAL AS ARR_TIME_LTC,&#xd;&#xa;--CAST&#x28;FROM_TZ &#x28;CAST&#x28;LF.SCH_ARRIVAL AS TIMESTAMP&#x29;,OFP.TIMEZONE_NAME&#x29; AT TIME ZONE &#x27;UTC&#x27; AS DATE&#x29; ARR_TIME_UTC,&#xd;&#xa;CAST&#x28;SYS_EXTRACT_UTC&#x28;FROM_TZ&#x28;CAST&#x28;LF.SCH_ARRIVAL AS TIMESTAMP&#x29;,NVL&#x28;TZ_OFFSET&#x28;OFP.TIMEZONE_NAME&#x29;,&#x27;&#x2b;00&#x3a;00&#x27;&#x29;&#x29;&#x29; AS DATE&#x29;ARR_TIME_UTC,&#xd;&#xa;--CASE WHEN LS.SELLING_CARRIER &#x3d; &#x27;FZ&#x27; THEN &#x27;738&#x27; ELSE NULL END EQUIPMENT_CODE,&#xd;&#xa;--CASE WHEN LS.FIRST_LEG_PFID &#x3d; LS.LAST_LEG_PFID THEN FS.AIRCRAFT_TYPE ELSE NULL END AS VERSION_CODE,&#xd;&#xa;TRIM&#x28;LF.FLIGHT_STATUS&#x29; AS STATUS--,&#xd;&#xa;--NULL AS SEAT_TEMPLATE_CODE,&#xd;&#xa;--NULL AS TAIL_NUMBER,&#xd;&#xa;--LF.LAST_MODIFIED_DATE,&#xd;&#xa;--LF.LAST_MODIFIED_DATE&#x2b;&#x28;4&#x2f;24&#x29; LAST_MODIFIED_DATE_LTC,&#xd;&#xa;--SYSDATE&#x2b;&#x28;4&#x2f;24&#x29; LAST_UPDATE_DATE for prod&#xd;&#xa;--SYSDATE LAST_UPDATE_DATE, &#x27;&#x27; OLD_STATUS&#xd;&#xa;--FROM P_FZ.LOGICAL_FLIGHTS LF&#xd;&#xa;FROM &#x28;SELECT &#x2a; FROM P_FZ.LOGICAL_FLIGHTS WHERE LOGICAL_FLIGHT_ID &#x3c;&#x3e; 8356615 AND DEPARTURE_DATE &#x3c;&#x3e; &#x27;23-APR-15&#x27;&#x29; LF&#xd;&#xa;INNER JOIN P_FZ.LOGICAL_SCHEDULES LS ON LF.LOGICAL_FLIGHT_ID &#x3d; LS.LOGICAL_FLIGHT_ID&#xd;&#xa;LEFT JOIN P_FZ.FLIGHT_SCHEDULES FS ON LS.FIRST_LEG_PFID &#x3d; FS.PHYSICAL_FLIGHT_ID&#xd;&#xa;LEFT JOIN P_FZ.AIRPORTS BDP ON LS.FROM_AIRPORT &#x3d; BDP.AIRPORT_CODE&#xd;&#xa;LEFT JOIN P_FZ.AIRPORTS OFP ON LS.TO_AIRPORT &#x3d; OFP.AIRPORT_CODE&#xd;&#xa;WHERE LS.FLIGHT_NUM NOT LIKE &#x27;&#x25;&#x2f;&#x25;&#x27; AND  LF.LAST_MODIFIED_DATE BETWEEN SYSDATE-&#x28;1&#x29; AND SYSDATE &#xd;&#xa;--WHERE LS.FLIGHT_NUM NOT LIKE &#x27;&#x25;&#x2f;&#x25;&#x27; AND LF.DEPARTURE_DATE BETWEEN &#x27;01-JUL-2015&#x27; AND &#x27;31-DEC-2015&#x27;&#xd;&#xa;ORDER BY TRIM&#x28;LS.FLIGHT_NUM&#x29;,TRIM&#x28;LS.FROM_AIRPORT&#x29;,TRIM&#x28;LS.TO_AIRPORT&#x29;,LF.DEPARTURE_DATE,LF.LOGICAL_FLIGHT_ID</sql>
